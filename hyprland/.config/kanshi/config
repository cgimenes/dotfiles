output eDP-1 scale 1
output DP-3 scale 1
output DP-4 scale 1
output "Some Other Company GTBZ 2525" mode 1920x1200

profile default {
        output eDP-1 enable
}

profile docked {
        output eDP-1 disable
        output DP-4 enable
}

profile docked2 {
        output eDP-1 disable
        output DP-3 enable
}
